{
    "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "jsx": "react-jsx",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["node", "react", "react-dom", "leaflet"]
    },
    "include": ["resources/js/**/*", "src/**/*"],
    "exclude": ["node_modules"]
  }
  